val a ++ b = a.concat(b);
val a +* b = if (b == 0) [] else a ++ (a +* (b - 1));

say([1, 2] ++ [3, 4]);
say([1, 2] ++ [3, 4] ++ [5, 6]);
say([1, 2, 3] +* 6);

say([1, 2] ++ [3, 4] +* 3);

val arr ∈ x = 
    if (arr.length() == 0) false 
    else 
        if arr.at(0) == x true 
        else arr.slice(1, arr.length()) ∈ x;

say();
say([1, 2, 3] ∈ 3);
say([1, 2, 3] ∈ 4);

